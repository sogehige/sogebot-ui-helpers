{
  "compilerOptions": {
    "target": "es6",
    "strict": true,
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "incremental": true,
    "removeComments": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["es2020", "dom"],
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "noResolve": false,
    "baseUrl": "./",
    "sourceMap": true,
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "@entity/*": [
        "../backend/src/database/entity/*"
      ]
    },
  },
  "include": [
    "*.ts",
    "**/*.ts",
    "../backend/d.ts/helpers/*"
  ],
  "exclude": [
    "src/bot",
    "node_modules"
  ]
}